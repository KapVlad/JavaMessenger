<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kap_vlad.android.javachat">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="Java Messenger"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Start">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignIn" />
        <activity android:name=".Register" />
        <activity android:name=".Enter" />
        <activity
            android:name=".MainSpace"
            android:label="Java Messenger"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".contacts.AddContact" />
        <activity android:name=".contacts.ShowContacts" />
        <activity android:name=".MyProfile"></activity>
    </application>

</manifest>